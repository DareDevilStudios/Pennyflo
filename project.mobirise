          "_name": "acontact1",
          "_sourceTheme": "expertm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"contact expertm5\" group=\"Contact\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <select title=\"Align\" name=\"contentAlign\">\n            <option value=\"justify-content-start\">Left</option>\n            <option value=\"justify-content-center\" selected>Center</option>\n            <option value=\"justify-content-end\">Right</option>\n        </select>\n        <input type=\"range\" inline title=\"Content Width\" name=\"contentWidth\" min=\"6\" max=\"12\" step=\"1\" value=\"9\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Image\" name=\"showImg\">\n        <input type=\"checkbox\" title=\"Image Border\" name=\"showImgBorder\" checked condition=\"showImg\">\n        <input type=\"color\" title=\"Image Border Color\" name=\"borderImgColor\" value=\"#1e2024\" condition=\"showImgBorder && showImg\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Border\" name=\"showBorder\" checked>\n        <input type=\"color\" title=\"Border Color\" name=\"borderColor\" value=\"#16181c\" condition=\"showBorder\">\n        <input type=\"range\" inline title=\"Contacts Count\" name=\"contactsCount\" min=\"0\" max=\"4\" step=\"1\" value=\"2\">\n        <input type=\"checkbox\" title=\"Contacts Title\" name=\"showCardTitle\" checked condition=\"contactsCount >0\">\n        <input type=\"checkbox\" title=\"Contacts Text\" name=\"showCardSubtitle\" condition=\"contactsCount >0\" checked=\"checked\">\n        <input type=\"checkbox\" title=\"Social\" name=\"showSocial\" checked>\n        <header condition=\"showSocial\">Social</header>\n        <input type=\"checkbox\" title=\"Social Title\" name=\"showSocialTitle\" condition=\"showSocial\">\n        <input type=\"color\" title=\"Social Color\" name=\"socialColor\" value=\"#C4CFDE\" condition=\"showSocial\">\n        <input type=\"color\" title=\"Social BG Color 1\" name=\"socialBgColor1\" value=\"#1e2024\" condition=\"showSocial\">\n        <input type=\"color\" title=\"Social BG Color 2\" name=\"socialBgColor2\" value=\"#23272b\" condition=\"showSocial\">\n        <input type=\"checkbox\" name=\"socialShadow\" title=\"Social Shadow\" condition=\"showSocial\" checked>\n        <input type=\"color\" title=\"Shadow Color 1\" name=\"socialShadow1\" value=\"#191b1f\" condition=\"socialShadow && showSocial\">\n        <input type=\"color\" title=\"Shadow Color 2\" name=\"socialShadow2\" value=\"#23272B\" condition=\"socialShadow && showSocial\">\n        <input type=\"range\" name=\"socialCount\" title=\"Social Count\" min=\"1\" max=\"6\" step=\"1\" value=\"3\" condition=\"showSocial\">\n        <header>Card Style</header>\n        <input type=\"color\" title=\"Card BG 1\" name=\"cardBg1\" value=\"#1e2024\">\n        <input type=\"color\" title=\"Card BG 2\" name=\"cardBg2\" value=\"#23272b\">\n        <input type=\"checkbox\" name=\"cardShadow\" title=\"Card Shadow\" checked>\n        <input type=\"color\" title=\"Shadow Color 1\" name=\"shadowColor1\" value=\"#191b1f\" condition=\"cardShadow\">\n        <input type=\"color\" title=\"Shadow Color 2\" name=\"shadowColor2\" value=\"#23272B\" condition=\"cardShadow\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@THEME_PATH@/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#212428\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@THEME_PATH@/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#212428\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <div class=\"row card-row {{contentAlign}}\">\n            <div class=\"item-card col-12 d-flex col-lg-{{contentWidth}}\">\n                <div class=\"item-wrap\">\n                    <div class=\"row m-0 align-items-start\">\n                        <div class=\"col-12 col-lg-6 flex-grow-1\" mbr-if=\"showImg || showTitle || showSubtitle || showSocial\">\n                            <div class=\"item-left-item\">\n                                <div class=\"image-wrapper\" mbr-if=\"showImg\">\n                                    <img src=\"@THEME_PATH@/components/_images/people/people5.jpg\" alt=\"Mobirise Website Builder\">\n                                </div>\n                                <div class=\"text-left-wrap\">\n                                    <h5 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">\n                                       <b>PennyFlo</b></h5>\n                                    <h5 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">secure trusted platform for banking applications</h5>\n                                    <div class=\"social-container\" mbr-if=\"showSocial\">\n                                        <p class=\"social-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSocialTitle\" data-app-selector=\".social-title\">\n                                            FIND WITH ME\n                                        </p>\n                                        <div class=\"social-list\">\n                                            <div class=\"soc-item\" mbr-if=\"socialCount>0\">\n                                                <span mbr-icon class=\"socicon-twitter socicon mbr-iconfont mbr-iconfont-social\"></span>\n                                            </div>\n                                            <div class=\"soc-item\" mbr-if=\"socialCount>1\">\n                                                <span mbr-icon class=\"socicon-facebook socicon mbr-iconfont mbr-iconfont-social\"></span>\n                                            </div>\n                                            <div class=\"soc-item\" mbr-if=\"socialCount>2\">\n                                                <span mbr-icon class=\"socicon-instagram socicon mbr-iconfont mbr-iconfont-social\"></span>\n                                            </div>\n                                            <div class=\"soc-item\" mbr-if=\"socialCount>3\">\n                                                <span mbr-icon class=\"socicon-youtube socicon mbr-iconfont mbr-iconfont-social\"></span>\n                                            </div>\n                                            <div class=\"soc-item\" mbr-if=\"socialCount>4\">\n                                                <span mbr-icon class=\"socicon-behance socicon mbr-iconfont mbr-iconfont-social\"></span>\n                                            </div>\n                                            <div class=\"soc-item\" mbr-if=\"socialCount>5\">\n                                                <span mbr-icon class=\"socicon-vimeo socicon mbr-iconfont mbr-iconfont-social\"></span>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n\n                        </div>\n                        <div class=\"col-12 col-lg-6 flex-grow-1 align-self-center\" mbr-if=\"contactsCount >0 &amp;&amp; (showCardTitle || showCardSubtitle)\">\n                            <div class=\"item-right-item\">\n                                <div class=\"contacts-container\">\n                                    <div class=\"contact-item\">\n                                        <p class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card-title\">\n                                            EMAIL\n                                        </p>\n                                        <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-text\">\n                                           <b>pennyflo@gmail.com</b></p>\n                                    </div>\n                                    <div class=\"contact-item\" mbr-if=\"contactsCount >1\">\n                                        <p class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card-title\">PHONE</p>\n                                        <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-text\">\n                                            <b>+91 6282238059</b></p>\n                                    </div>\n                                </div>\n                                <div class=\"contacts-container\" mbr-if=\"contactsCount >2\">\n                                    <div class=\"contact-item\" mbr-if=\"contactsCount >2\">\n                                        <p class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card-title\">\n                                            PHONE\n\n                                        </p>\n                                        <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-text\">\n                                            <b>(111) 222-4321</b>\n                                        </p>\n                                    </div>\n                                    <div class=\"contact-item\" mbr-if=\"contactsCount >3\">\n                                        <p class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card-title\">\n                                            LOCATION\n\n                                        </p>\n                                        <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-text\">\n                                            <b>Brooklyn, New York 14241</b>\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
